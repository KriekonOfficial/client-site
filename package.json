{
  "name": "kriekon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^4.4.1",
    "@vue/cli-service": "^4.4.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "core-js": "^2.6.11",
    "crypto-js": "^4.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.26.7",
    "sass-loader": "^7.3.1",
    "vue": "^2.6.11",
    "vue-animejs": "^2.1.1",
    "vue-axios": "^2.1.5",
    "vue-router": "^3.3.2",
    "vue-template-compiler": "^2.6.11",
    "vue-videobg": "^2.0.0",
    "vuex": "^3.4.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "indent": [
        1,
        "tab"
      ],
      "no-tabs": 0,
      "brace-style": [
        2,
        "allman"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
